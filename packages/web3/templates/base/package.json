{
  "name": "my-dapp-template",
  "version": "0.1.0",
  "license": "GPL",
  "config": {
    "alephium_version": "1.5.0-rc6"
  },
  "scripts": {
    "build": "rm -rf dist && npx tsc --build .",
    "start-devnet": "node scripts/start-devnet.js ${npm_package_config_alephium_version}",
    "restart-devnet": "npm run start-devnet",
    "stop-devnet": "node scripts/stop-devnet.js"
  },
  "dependencies": {
    "@alephium/web3": "0.2.0-rc.7"
  },
  "devDependencies": {
    "@types/elliptic": "^6.4.13",
    "@types/node": "^16.7.8",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "bracketSameLine": false,
    "trailingComma": "none"
  }
}
